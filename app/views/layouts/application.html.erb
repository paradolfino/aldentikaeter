<!DOCTYPE html>
<html>
  <head>
    <title>Aldentikaeter</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>
    <% if current_user %>
      <h3><%= link_to "Log Out", logout_path %></h3>
    <% else %>
      <h3>Not a member? <%= link_to "Sign Up!", signup_path %></h3> or <%= link_to "Log In", login_path %>
    <% end %>
  </body>
</html>
