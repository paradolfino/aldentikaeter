<!DOCTYPE html>
<html>
  <head>
    <title>Aldentikaeter</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if current_user %>
      <h3><%= link_to "Log Out", logout_path, method: "delete" %></h3>
    <% else %>
      <h3>Not a member? <%= link_to "Sign Up!", signup_path %></h3> or <%= login_link_to "Login" %>
    <% end %>
    <%= yield %>
    
  </body>
</html>
